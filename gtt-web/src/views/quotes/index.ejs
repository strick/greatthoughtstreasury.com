
<div class="col-lg-12"><h1><%=title%></h1></div>
<%- include('../partials/pagination.ejs', {page: 'quotes'}); %>
<% quotes.forEach(function(quote){ %>
    <div class="col-lg-3 col-md-6 mb-4">
        <div class="card h-100">       
          <div class="card-body">          
            <p class="card-text"><%= quote.quote %></p>
          </div>
          <div class="card-footer">
            <a href="/quotes" class="btn btn-primary">View Similar</a>
          </div>
        </div>
      </div> 
    <% }) %>
    <%- include('../partials/pagination.ejs', {page: 'quotes'}); %>