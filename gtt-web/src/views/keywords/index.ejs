
<div class="col-lg-12"><h1><%=title%></h1></div>
<%- include('../partials/pagination.ejs', {page: 'keywords'}); %>
<% keywords.forEach(function(keyword){ %>
    <div class="col-lg-3 col-md-6 mb-4">
        <div class="card h-100">       
          <div class="card-body">          
            <p class="card-text"><%= keyword.keyword %></p>
          </div>
          <div class="card-footer">
            <a href="/keywords/single/<%= keyword._id %>" class="btn btn-primary">View Quotes</a>
          </div>
        </div>
      </div> 
    <% }) %>  
    <%- include('../partials/pagination.ejs', {page: 'keywords'}); %>